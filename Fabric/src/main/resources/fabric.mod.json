{
    "schemaVersion": 1,
    "id": "armoreablemobs",
    "version": "${version}",
  
    "name": "ArmoreableMobs",
    "description": "A mod that allows you to give any item to any entity when it spawns using CraftTweaker.",
    "authors": [
      "Buuz135", "Witixin"
    ],
    "contact": {
      "sources": "https://github.com/Buuz135/ArmoreableMobs"
    },
  
    "license": "MIT",
  
    "environment": "*",
    "entrypoints": {
      "main": [
        "net.witixin.armoreablemobs.Reference"
      ]
    },
    "mixins": [
      "armoreablemobs.mixins.json"
    ],
  
    "depends": {
      "fabricloader": ">=0.14.9",
      "minecraft": "1.19.2",
      "java": ">=17",
      "crafttweaker" : ">=10.0.14"
    }
  }
  